  "files": [
    "build",
    "LICENSE",
    "README.md",
    "!*.js.map"
  ],
  "devDependencies": {
    "@magicspace/configs": "^0.1.49",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.3",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
